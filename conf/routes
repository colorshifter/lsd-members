# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /                           controllers.MembershipController.index
GET     /alt                        controllers.MembershipController.alt
POST    /sign-up                    controllers.MembershipController.signup
POST    /sign-up-alt                controllers.MembershipController.signupAlt
GET     /thank-you                  controllers.MembershipController.thankYou

# Admin

GET     /admin                      controllers.AdminController.index
GET     /settings                   controllers.AdminController.settings

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
